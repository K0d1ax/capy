{
  "name": "landingstudio",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
