    "lint": "turbo run lint",
    "test": "turbo run test && pnpm --filter @landingstudio/frontend test:e2e",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "prettier": "^3.3.3",
    "turbo": "^2.1.1",
    "typescript": "^5.6.3",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14"
  }
}
